{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/horario.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/horario.png",
      "resizeMode": "native",
      "backgroundColor": "black"
      
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/horario.png",
        "backgroundColor": "black"
      }

    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
